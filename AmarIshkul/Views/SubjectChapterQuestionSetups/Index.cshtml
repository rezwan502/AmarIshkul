@model IEnumerable<AmarIshkul.Entity.SubjectChapterQuestionSetup>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubjectChapterWiseDetails.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfQuestion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PerQuestionMark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalMarks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HighestMarks)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubjectChapterWiseDetails.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfQuestion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PerQuestionMark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalMarks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HighestMarks)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
